{
  "name": "@mirazmac/eagerform",
  "version": "1.0.0",
  "description": "A vanilla javascript form validation library based on HTML5 constraints with support for custom rules, messages and localizations.",
  "main": "dist/EagerForm.umd.min.js",
  "module": "dist/EagerForm.esm.min.js",
  "unpkg": "dist/EagerForm.umd.min.js",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.js",
    "test": "jest --config jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "clean": "rimraf dist coverage",
    "prepare": "npm-run-all clean lint test build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MirazMac/EagerForm"
  },
  "keywords": [],
  "author": "Miraz Mac <mirazmac@gmail.com> (https://github.com/MirazMac)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MirazMac/EagerForm/issues"
  },
  "homepage": "https://github.com/MirazMac/EagerForm",
  "devDependencies": {
    "@babel/core": "~7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "~7.12.1",
    "@babel/preset-env": "~7.12.11",
    "@babel/register": "~7.12.10",
    "@babel/runtime": "~7.14.0",
    "@rollup/plugin-babel": "~5.2.2",
    "@rollup/plugin-commonjs": "~17.0.0",
    "@rollup/plugin-node-resolve": "~11.1.0",
    "core-js": "~3.13.1",
    "cross-env": "~7.0.3",
    "eslint": "~7.18.0",
    "jest": "~26.6.3",
    "npm-run-all": "~4.1.5",
    "regenerator-runtime": "~0.13.7",
    "rimraf": "~3.0.2",
    "rollup": "~2.38.0",
    "rollup-plugin-livereload": "~2.0.0",
    "rollup-plugin-serve": "~1.1.0",
    "rollup-plugin-terser": "~7.0.2"
  },
  "dependencies": {
    "lodash.debounce": "~4.0.8"
  }
}
